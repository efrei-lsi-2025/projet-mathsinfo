{
  "name": "@metro-boulot-dodo/proto",
  "version": "1.0.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "exports": {
    ".": "./src/index.ts"
  },
  "scripts": {
    "generate-index": "find ./src/generated -name '*.ts' ! -name 'index.ts' -print | sed 's|^\\./src/|export * from \"./|' | sed 's|.ts$|\";|' > ./src/index.ts",
    "clean": "rimraf src/generated && mkdir -p src/generated",
    "build": "npm run clean && npx protoc -I=protos protos/*.proto --ts_out=src/generated --ts_opt server_grpc1,client_none,optimize_code_size"
  },
  "devDependencies": {
    "@protobuf-ts/plugin": "^2.8.0",
    "rimraf": "^6.0.1"
  }
}
